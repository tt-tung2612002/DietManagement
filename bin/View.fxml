<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="myAnchor" prefHeight="800.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ViewController">
   <children>
      <TextField fx:id="amountField" alignment="CENTER" focusTraversable="false" layoutX="550.0" layoutY="45.0" prefHeight="41.0" prefWidth="156.0" promptText="Amount">
         <font>
            <Font name="Fira Code Retina" size="20.0" />
         </font>
      </TextField>
      <Button fx:id="clearButton" focusTraversable="false" layoutX="1065.0" layoutY="45.0" mnemonicParsing="false" onAction="#clearItem" text="Clear" textAlignment="CENTER" textOverrun="WORD_ELLIPSIS">
         <font>
            <Font name="Fira Code Retina" size="20.0" />
         </font>
      </Button>
      <Button fx:id="addButton" focusTraversable="false" layoutX="838.0" layoutY="45.0" mnemonicParsing="false" onAction="#addItem" text="Add" textAlignment="CENTER" textOverrun="WORD_ELLIPSIS">
         <font>
            <Font name="Fira Code Retina" size="20.0" />
         </font>
      </Button>
      <Button fx:id="deleteButton" focusTraversable="false" layoutX="934.0" layoutY="45.0" mnemonicParsing="false" onAction="#deleteItem" text="Delete" textAlignment="CENTER" textOverrun="WORD_ELLIPSIS">
         <font>
            <Font name="Fira Code Retina" size="20.0" />
         </font>
      </Button>
      <Button fx:id="saveButton" focusTraversable="false" layoutX="1188.0" layoutY="45.0" mnemonicParsing="false" text="Save" textAlignment="CENTER" textOverrun="WORD_ELLIPSIS">
         <font>
            <Font name="Fira Code Retina" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
